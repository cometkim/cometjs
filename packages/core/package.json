{
  "name": "@cometjs/core",
  "description": "cometjs core utilities",
  "version": "2.3.1",
  "license": "MIT",
  "homepage": "https://github.com/cometkim/cometjs/tree/main/packages/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/cometjs.git",
    "directory": "packages/core"
  },
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim"
  },
  "sideEffects": false,
  "main": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.mjs",
        "require": "./lib/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "ios >= 13"
  ],
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "nanobundle build",
    "prepack": "rm -rf lib && yarn build"
  },
  "peerDependencies": {
    "typescript": "^4.5.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "nanobundle": "^1.0.0-rc.8",
    "typescript": "^5.0.4",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "dependencies": {
    "tsconfck": "^2.0.1"
  }
}
