{
  "name": "@cometjs/core",
  "description": "cometjs core utilities",
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "https://github.com/cometkim/cometjs/tree/main/packages/core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/cometjs.git",
    "directory": "packages/core"
  },
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "import": "./lib/index.mjs",
        "require": "./lib/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "ios >= 13"
  ],
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "nanobundle build",
    "prepack": "rm -rf lib && yarn build"
  },
  "peerDependencies": {
    "typescript": "^4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "nanobundle": "^0.0.27",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  }
}
