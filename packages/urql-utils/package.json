{
  "name": "@cometjs/urql-utils",
  "description": "cometjs utility collection for urql",
  "version": "2.0.3",
  "license": "MIT",
  "homepage": "https://github.com/cometkim/cometjs/tree/main/packages/urql-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/cometjs.git",
    "directory": "packages/urql-utils"
  },
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./lib/index.mjs",
    "exports": {
      ".": {
        "import": "./lib/index.mjs",
        "require": "./lib/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsc --emitDeclarationOnly && nanobundle build --dts=false",
    "prepack": "rm -rf lib && yarn build"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "urql": "^2.0.0"
  },
  "dependencies": {
    "@cometjs/core": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack-env": "^1.16.0",
    "c8": "^7.11.0",
    "graphql": "^15.5.0",
    "nanobundle": "^0.0.27",
    "react": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.6.2",
    "urql": "^2.0.1",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "wonka": "^4.0.15"
  }
}
