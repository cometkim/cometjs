{
  "name": "@cometjs/relay-utils",
  "description": "cometjs utility collection for Relay",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/cometkim/cometjs/tree/main/packages/relay-utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/cometjs.git",
    "directory": "packages/relay-utils"
  },
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "type": "module",
    "main": "./lib/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsc --emitDeclarationOnly && nanobundle build --dts=false",
    "prepack": "rm -rf lib && yarn build"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "@urlpack/json": "^1.1.0"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "nanobundle": "^0.0.27",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  }
}
