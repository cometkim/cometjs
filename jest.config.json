{
  "verbose": true,
  "transform": {
    "\\.[jt]sx?$": ["babel-jest", {
      "presets": [
        "@babel/preset-typescript",
        "@babel/preset-react",
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ]
    }]
  },
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    ".yarn",
    "node_modules",
    "__mock__"
  ]
}
